{
  "name": "quicksend-code2ai",
  "displayName": "QuickSend Code2AI",
  "description": "Easily share code files with ChatGPT, Claude, Gemini & other AI assistants",
  "version": "1.0.0",
  "publisher": "ParekhDhruv",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other",
    "Formatters",
    "Snippets"
  ],
  "keywords": [
    "ai",
    "chatgpt",
    "claude",
    "gemini",
    "llm",
    "code sharing",
    "clipboard",
    "file share",
    "code formatter",
    "productivity",
    "quicksend code2ai",
    "quicksend code to ai"
  ],
  "icon": "images/icon.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "quicksend-code2ai.shareFiles",
        "title": "Share Files with AI Assistant"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "quicksend-code2ai.shareFiles",
          "group": "ParekhDhruv@1"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhruv0127/QuickSend-All-Files"
  }
}